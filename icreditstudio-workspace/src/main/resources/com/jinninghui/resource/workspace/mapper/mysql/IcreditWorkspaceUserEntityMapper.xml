<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jinninghui.datasphere.icreditstudio.workspace.mapper.IcreditWorkspaceUserMapper">

    <select id="getWorkspaceByUserId" parameterType="java.lang.String" resultType="map">
        select DISTINCT w.id, w.name
        from icredit_workspace_user u,
             icredit_workspace w
        <where>
            <if test="id != null and id != '' ">
                and u.user_id = #{id}
            </if>
            AND u.space_id = w.id
            AND w.del_flag = 'N'
        </where>
          order by w.update_time desc;
    </select>

    <select id="getUserListById" parameterType="java.lang.String"
            resultType="com.jinninghui.datasphere.icreditstudio.workspace.entity.IcreditWorkspaceUserEntity">
        select *
        from icredit_workspace_user
        where space_id = #{id};
    </select>
</mapper>
